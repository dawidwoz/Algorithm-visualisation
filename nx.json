{
  "npmScope": "major-project",
  "affected": { "defaultBase": "master" },
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": { "dependencies": "*", "devDependencies": "*" },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": { "cacheableOperations": ["build", "lint", "test", "e2e"] }
    }
  },
  "projects": {
    "visualisation": { "tags": [] },
    "visualisation-e2e": { "tags": [], "implicitDependencies": ["visualisation"] },
    "stack": { "tags": [] },
    "common": { "tags": [] },
    "queue": { "tags": [] },
    "queue-priority": { "tags": [] },
    "hash-function": { "tags": [] },
    "binary-tree": { "tags": [] },
    "markov-model": { "tags": [] }
  }
}
